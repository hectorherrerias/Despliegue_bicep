{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "resourceGroupName": {
        "value": "myResourceGroup"
      },
      "location": {
        "value": "North Europe"
      },
      "vnetName": {
        "value": "myVnet"
      },
      "vnetAddressPrefix": {
        "value": "10.0.0.0/16"
      },
      "subnets": {
        "value": [
          {
            "name": "subnet1",
            "addressPrefix": "10.0.1.0/24"
          },
          {
            "name": "subnet2",
            "addressPrefix": "10.0.2.0/24"
          }
        ]
      },
      "keyVaultName": {
        "value": "myKeyVault"
      },
      "aksClusterName": {
        "value": "myAKSCluster"
      },
      "aksNodeCount": {
        "value": 3
      },
      "aksNodeSize": {
        "value": "Standard_DS2_v2"
      },
      "servicePrincipalClientId": {
        "value": "<your-service-principal-client-id>"
      },
      "servicePrincipalClientSecret": {
        "value": "<your-service-principal-client-secret>" 
      },
      "sqlServerName": {
        "value": "mySqlServer"
      },
      "sqlAdminUsername": {
        "value": "sqlAdmin"
      },
      "sqlAdminPassword": {
        "value": "YourSecurePassword123!" 
      },
      "sqlDbName": {
        "value": "myDatabase"
      },
      "serviceBusNamespaceName": {
        "value": "myServiceBusNamespace"
      },
      "queueName": {
        "value": "myQueue"
      },
      "appServicePlanName": {
        "value": "myAppServicePlan"
      },
      "appName": {
        "value": "myWebApp"
      }
    }
  }
  